{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "client",
            "type": "debugpy",
            "request": "launch",
            "module": "fastapi",
            "args": [
                "dev",
                "web.py"
            ],
            "jinja": true,
            "cwd": "${workspaceFolder}/client/"
        },
        {
            "name": "chrome",
            "request": "launch",
            "type": "chrome",
            "url": "",
            "webRoot": "${workspaceFolder}/frontend/"
        },
        {
            "name": "seed-database",
            "type": "debugpy",
            "request": "launch",
            "program": "seed.py",
            "cwd": "${workspaceFolder}/database-seed/",
            "console": "integratedTerminal",
            "args": ["<user register endpoint>", "<tweet endpoint>", "<amount of users>", "<tweet per user>"]
        },
        {
            "name": "controller",
            "type": "debugpy",
            "request": "launch",
            "program": "controller.py",
            "cwd": "${workspaceFolder}/controller/",
            "console": "integratedTerminal",
            "args": ["http://10.7.7.127:8000", "schemas/test.json", "http://10.7.7.167:5000/", "results"]
        }
    ]
}